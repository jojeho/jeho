    std::enable_if_t<boost::hana::Struct<T>::value,void>
    go(Stream &ar ,T& in)
    {
      
    }


    template <typename Stream ,typename T>
    std::enable_if_t<!boost::hana::Struct<T>::value, void>
    go(Stream &ar ,T& in)
    {
      
    };

  }}
